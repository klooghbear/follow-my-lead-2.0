{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [
            "error", 
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "object-curly-spacing": [
            "error", 
            "always"
        ],
        "new-cap": [
            "error", 
            { 
                "capIsNew": false 
            }
        ],
        "require-jsdoc": "off",
        "react/no-unescaped-entities": 0,
        "react/prop-types": 0
    }
}
